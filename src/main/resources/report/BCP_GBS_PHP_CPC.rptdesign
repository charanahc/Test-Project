            <xml-property name="queryText"><![CDATA[SELECT pdb.PROJECT_ID,pdb.PROJECT_NAME,pdb.PMP_CLIENT_NAME,pdb.PROJECT_MANAGER,dcd.VERSION_NO,dcd.AUTHOR_NM,VARCHAR_FORMAT(dcd.CREATED_TMSP,'yyyy-MM-dd HH:mm') AS CREATED_TMSP,dcd.VERSION_DT,dcd.SUMRY_OF_CHANGE,pdb.PAL 
FROM BCMPHP.PROJECT_DB pdb 
INNER JOIN BCMPHP.BCM_REQUEST_DRAFT brd on pdb.PROJECT_ID = brd.PROJ_ACC_ID
INNER JOIN BCMPHP.DOCUMENT_CTRL_DRAFT dcd on dcd.REQ_ID = brd.REQ_ID
WHERE pdb.PROJECT_ID=?]]></xml-property>
