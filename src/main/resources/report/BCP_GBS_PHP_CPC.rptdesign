                                <property name="paddingRight">3pt</property>
                                <text id="7048">
                                    <property name="contentType">html</property>
                                    <text-property name="content"><![CDATA[<B>Business Continuity Plan (BCP):</B> A clearly defined and documented plan for use at the time of a Disaster that addresses procedures for responding to a Disaster. This plan also addresses key personnel, resources, services, actions, and roles and responsibilities required to manage the Business Continuity process.   ]]></text-property>
                                </text>
                            </cell>
                            <cell id="7099">
                                <property name="borderBottomStyle">solid</property>
                                <property name="borderBottomWidth">thin</property>
                                <property name="borderLeftStyle">solid</property>
